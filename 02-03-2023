  487  grep -i -e "linux" -i -e "credit" 
  488  grep -i -e "linux" -i -e "credit" exp4
  489  grep -i -e "linux"  "credit" exp4
  490  grep -i -e "linux" -e "credit" exp4
  491  grep -ie "linux" -ie "credit" exp4 =============> works with this format too
